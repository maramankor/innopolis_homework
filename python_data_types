number = int(input("Введите число: ")
last_digit = number % 10
last_two_digits = number % 100
    if last_digit = 1 and last_two_digits !=11:
ending = "рубль"
    elif 2 <= last_digit <= 4 and not (12 <= last_two_digits <= 14):
ending = "рубля"
    else:
ending = "рублей"
result = f"{number} {ending}"
print(result)
